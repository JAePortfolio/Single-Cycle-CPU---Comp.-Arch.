`timescale 1 ps/ 1 ps
module Arena_bitwise_setLessThanSigned_vlg_sample_tst(
	Arena_a_sltIN,
	Arena_b_sltIN,
	Arena_sltSelect,
	sampler_tx
);
input [5:0] Arena_a_sltIN;
input [5:0] Arena_b_sltIN;
input  Arena_sltSelect;
output sampler_tx;

reg sample;
time current_time;
always @(Arena_a_sltIN or Arena_b_sltIN or Arena_sltSelect)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module Arena_bitwise_setLessThanSigned_vlg_check_tst (
	Arena_result_sltOUT,
	sampler_rx
);
input  Arena_result_sltOUT;
input sampler_rx;

reg  Arena_result_sltOUT_expected;

reg  Arena_result_sltOUT_prev;

reg  Arena_result_sltOUT_expected_prev;

reg  last_Arena_result_sltOUT_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:1] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 1'b1;
end

// update real /o prevs

always @(trigger)
begin
	Arena_result_sltOUT_prev = Arena_result_sltOUT;
end

// update expected /o prevs

always @(trigger)
begin
	Arena_result_sltOUT_expected_prev = Arena_result_sltOUT_expected;
end



// expected Arena_result_sltOUT
initial
begin
	Arena_result_sltOUT_expected = 1'bX;
end 
// generate trigger
always @(Arena_result_sltOUT_expected or Arena_result_sltOUT)
begin
	trigger <= ~trigger;
end

always @(posedge sampler_rx or negedge sampler_rx)
begin
`ifdef debug_tbench
	$display("Scanning pattern %d @time = %t",i,$realtime );
	i = i + 1;
	$display("| expected Arena_result_sltOUT = %b | ",Arena_result_sltOUT_expected_prev);
	$display("| real Arena_result_sltOUT = %b | ",Arena_result_sltOUT_prev);
`endif
	if (
		( Arena_result_sltOUT_expected_prev !== 1'bx ) && ( Arena_result_sltOUT_prev !== Arena_result_sltOUT_expected_prev )
		&& ((Arena_result_sltOUT_expected_prev !== last_Arena_result_sltOUT_exp) ||
			on_first_change[1])
	)
	begin
		$display ("ERROR! Vector Mismatch for output port Arena_result_sltOUT :: @time = %t",  $realtime);
		$display ("     Expected value = %b", Arena_result_sltOUT_expected_prev);
		$display ("     Real value = %b", Arena_result_sltOUT_prev);
		nummismatches = nummismatches + 1;
		on_first_change[1] = 1'b0;
		last_Arena_result_sltOUT_exp = Arena_result_sltOUT_expected_prev;
	end

	trigger <= ~trigger;
end
initial 

begin 
$timeformat(-12,3," ps",6);
#160000;
if (nummismatches > 0)
	$display ("%d mismatched vectors : Simulation failed !",nummismatches);
else
	$display ("Simulation passed !");
$finish;
end 
endmodule

